  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src && cp -r ./src/templates ./build/src/templates",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/server.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/server.js",
    "debug": "NODE_ENV=debug nodemon --exec ./node_modules/.bin/babel-node ./src/server.js",
    "lint": "eslint --ext js,ts src/",
    "lint:fix": "eslint --fix --ext js,ts src/",
    "prettier": "prettier --check \"src/**/(*.js|*.ts)\"",
    "prettier:fix": "prettier --write \"src/**/(*.js|*.ts)\"",
    "docker": "docker-compose -f docker-compose.yml up -d",
    "prepare": "husky install"
